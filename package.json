{
  "name": "@pikostudio/command.ts",
  "description": "Discord.js command framework for typescript.",
  "version": "2.0.13",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "discord.js": "discordjs/discord.js",
    "discord.js-docgen": "discordjs/discord.js-docgen",
    "prettier": "^2.2.1",
    "ts-node": "^10.1.0",
    "typedoc": "^0.20.35",
    "typedoc-plugin-markdown": "^3.6.1",
    "vuepress": "^1.8.2"
  },
  "dependencies": {
    "@types/node": "^14.14.37",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pikokr/command.ts-v2"
  },
  "scripts": {
    "prepack": "tsc",
    "docs:dev": "typedoc --out docs/docs/api src/index.ts && vuepress dev docs",
    "docs:build": "typedoc --out docs/docs/api src/index.ts && vuepress build docs"
  }
